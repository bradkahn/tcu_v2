# internal
NET "clk_IN"            LOC = "V10" | IOSTANDARD = "LVCMOS33";   #Bank = 2, pin name = IO_L30N_GCLK0_USERCCLK,            Sch name = GCLK

NET "rst_IN"         LOC = T12   | IOSTANDARD = "LVCMOS33" | PULLDOWN;   #Bank = 2, Pin name = IO_L19P, Sch name = JA1
NET "trigger_IN"     LOC = V12   | IOSTANDARD = "LVCMOS33" | PULLDOWN;   #Bank = 2, Pin name = IO_L19N, Sch name = JA2
NET "bias_x_OUT"     LOC = N10   | IOSTANDARD = "LVCMOS33";   #Bank = 2, Pin name = IO_L20P, Sch name = JA3
NET "bias_l_OUT"     LOC = P11   | IOSTANDARD = "LVCMOS33";   #Bank = 2, Pin name = IO_L20N, Sch name = JA4
NET "pol_tx_x_OUT"   LOC = M10   | IOSTANDARD = "LVCMOS33";   #Bank = 2, Pin name = IO_L22P, Sch name = JA7
NET "pol_tx_l_OUT"   LOC = N9    | IOSTANDARD = "LVCMOS33";   #Bank = 2, Pin name = IO_L22N, Sch name = JA8
NET "pol_rx_l_OUT"   LOC = U11   | IOSTANDARD = "LVCMOS33";   #Bank = 2, Pin name = IO_L23P, Sch name = JA9
NET "pri_OUT"        LOC = V11   | IOSTANDARD = "LVCMOS33";   #Bank = 2, Pin name = IO_L23N, Sch name = JA10
