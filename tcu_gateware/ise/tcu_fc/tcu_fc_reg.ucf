NET "CLK_I" TNM_NET = CLK_I;
TIMESPEC TS_CLK_I = PERIOD "CLK_I" 11 ns HIGH 50%;
NET "clk_IN" TNM_NET = clk_IN;
TIMESPEC TS_clk_IN = PERIOD "clk_IN" 10 ns HIGH 50%;
NET "pri_OUT" OFFSET = OUT 10 ns AFTER "clk_IN";

